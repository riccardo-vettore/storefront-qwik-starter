{
	"name": "storefront-qwik-starter",
	"description": "File based routing and MDX support Vite.js tooling. Cloudflare Pages. Prettier code formatter. Tailwind CSS framework.",
	"scripts": {
		"build": "qwik build",
		"build.client": "vite build",
		"build.preview": "vite build --ssr src/entry.preview.tsx",
		"build.server": "vite build -c adaptors/cloudflare-pages/vite.config.ts",
		"build.types": "tsc --incremental --noEmit",
		"deploy": "wrangler pages dev ./dist",
		"dev": "vite --mode ssr",
		"dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
		"fmt": "prettier --write .",
		"fmt.check": "prettier --check .",
		"lint": "eslint \"src/**/*.ts*\"",
		"prepare": "husky install",
		"preview": "qwik build preview && vite preview --open",
		"start": "vite --open --mode ssr",
		"qwik": "qwik",
		"generate": "graphql-codegen --config codegen.yml"
	},
	"devDependencies": {
		"@builder.io/qwik": "0.20.1",
		"@builder.io/qwik-city": "0.5.2",
		"@graphql-codegen/cli": "^3.2.1",
		"@graphql-codegen/introspection": "^3.0.1",
		"@graphql-codegen/typescript": "^3.0.1",
		"@graphql-codegen/typescript-operations": "^3.0.1",
		"@tailwindcss/forms": "^0.5.3",
		"@types/eslint": "8.21.1",
		"@types/node": "latest",
		"@typescript-eslint/eslint-plugin": "5.54.0",
		"@typescript-eslint/parser": "5.54.0",
		"autoprefixer": "10.4.13",
		"eslint": "8.35.0",
		"eslint-plugin-qwik": "0.20.1",
		"husky": "^8.0.3",
		"node-fetch": "3.3.0",
		"postcss": "8.4.21",
		"prettier": "2.8.4",
		"pretty-quick": "^3.1.3",
		"tailwindcss": "3.2.7",
		"typescript": "4.9.5",
		"vite": "4.1.4",
		"vite-tsconfig-paths": "4.0.5",
		"wrangler": "latest"
	},
	"engines": {
		"node": ">=16"
	},
	"homepage": "https://qwik.builder.io/",
	"license": "",
	"private": true,
	"type": "module",
	"dependencies": {
		"@graphql-codegen/typescript-generic-sdk": "^3.1.0",
		"graphql": "^16.6.0",
		"graphql-tag": "^2.12.6",
		"undici": "^5.20.0",
		"zod": "^3.20.6"
	}
}
